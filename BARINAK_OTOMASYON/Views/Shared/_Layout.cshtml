<!DOCTYPE html>
<html lang="en">
<head>
    <title>Bootstrap Example</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>

    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" href="#">BARINAK OTOMASYONU</a>
            </div>
            <ul class="nav navbar-nav">

                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="">HAYVAN İŞLEMLERİ <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li><a href="/Hayvan/Index">HAYVAN LİSTESİ</a></li>
                        <li><a href="/Hayvan/Ekle">HAYVAN EKLE</a></li>

                    </ul>
                </li>
                <li class="dropdown">
                    <a class="dropdown-toggle" data-toggle="dropdown" href="">SAHİP İŞLEMLERİ <span class="caret"></span></a>
                    <ul class="dropdown-menu">
                        <li><a href="/Sahip/Index">SAHİP LİSTESİ</a></li>
                        <li><a href="/Sahip/Ekle">SAHİP EKLE</a></li>

                    </ul>
                </li>
            </ul>
            <ul class="nav navbar-nav navbar-right">

                <li><button type="button" onclick="location='/Admin/Login/LogOff'" class="btn btn-primary btn-lg">ÇıkışYap</button></li>
            </ul>
        </div>
    </nav>


    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>


    </div>

</body>
</html>




